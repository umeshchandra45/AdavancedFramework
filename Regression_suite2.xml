<?xml version="1.0" encoding="UTF-8"?>
<suite name="Harmonic Regression Suite" parallel="methods"
	thread-count="1" configfailurepolicy="skip">
	<listeners>
		<listener
			class-name="com.Forsys.api.report.ExtentReportListener" />
	</listeners>
	<parameter name="archive.confiance.report" value="true" />
	<parameter name="confiance.report.env" value="Harmonic" />
	<parameter name="confiance.report.host"
		value="Harmonic Regression" />
	<parameter name="confiance.report.reportheader"
		value="Harmonic Automation Report" />
	  <test name="Harmonic Regression Test" configfailurepolicy="skip">  
			<classes>
		      <class name="com.framework.apiautomation.test.VOS360_CancellingInvoices" /> 
		      
		  </classes>
	</test> 
</suite>